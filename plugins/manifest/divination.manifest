{
	"pluginName": "divination",
	"pluginAlias": "抽签",
	"script": "divination.js",
	"description": "抽签功能，随机返回一张签",
	"author": "Runc2333",
	"defaultState": "enable",
	"switchable": true,
	"globalConfigurations": false,
	"groupSensitive": false,
	"webConsole": false,
	"message": [{
		"type": [
			"GROUP_MESSAGE", "PRIVATE_MESSAGE", "DISCUSS_MESSAGE"
		],
		"mode": "regexp",
        "regexp": "^抽签$|^占卜$",
		"handler": "divination",
		"priority": 0,
		"skipable": true,
		"silent": false
	}]
}